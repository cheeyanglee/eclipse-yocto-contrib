------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

if [$sdk_path | not-eq "" | $build_tree_path | not-eq ""] {
  //create_new_profile
  get-preferences-menu | click
  with [get-window Preferences] {
      get-tree | select "Yocto Project \\(alpha\\)"
      get-group "Cross development profile" | get-button "New..." | click
      with [get-window "Create new profile"] {
          get-editbox -after [get-label "Enter new profile name"] | set-text $temporary_profile_name1
          get-button OK | click
      }
      if [$sdk_path | eq ""] {
        get-group Toolchain | get-button "Build directory:" | click
      }
  }

  if [$sdk_path | not-eq ""] {
    set-dialog-result Folder $sdk_path
    with [get-window Preferences] {
        get-group Toolchain | get-button "Edit..." | click
    }
  }-else{
    set-dialog-result Folder $build_tree_path
    with [get-window Preferences] {
        get-group Toolchain | get-button "Edit..." | click
    }
  }

  with [get-window Preferences] {
      get-button Apply | click
      get-button "Apply and Close" | click
  }

  //rename_profile
  get-preferences-menu | click
  with [get-window Preferences] {
      get-group "Cross development profile" | get-button "Rename..." | click
      with [get-window "Rename profile"] {
          get-editbox -after [get-label "Enter new name"] | set-text $temporary_profile_name2
          get-button OK | click
      }
      get-button Apply | click
      get-button "Apply and Close" | click
  }

  //remove_profile
  get-preferences-menu | click
  with [get-window Preferences] {
      get-tree | select "Yocto Project \\(alpha\\)"
      with [get-group "Cross development profile"] {
          get-combo -after [get-label "Profile:"] | select $temporary_profile_name2
          get-button Remove | click
          get-combo -after [get-label "Profile:"] | verify-error {select $temporary_profile_name2}
      }
  }
}-else{
  echo "Test case \"Create Rename and delete cross development profile\" skipped. Parameter sdk_path and build_tree_path not set."
}

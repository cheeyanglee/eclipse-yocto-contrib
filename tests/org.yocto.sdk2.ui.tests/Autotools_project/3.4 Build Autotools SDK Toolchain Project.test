------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

if [$sdk_path | not-eq ""] {
	get-menu -path "File/New/C\\/C++ Project" | click
	get-window "New C/C++ Project" | get-table | select [get-item -path "" -index 1] | click -default
	with [get-window -class WizardDialog] {
	    get-tree | select "GNU Autotools/Hello World ANSI C Autotools Project"
	    get-editbox -after [get-label "Project name:"] | set-text $autotools_sdk_toolchain_project_name
	    get-button Finish | click
	}
	with [get-view "Project Explorer" | get-tree] {
	    select $autotools_sdk_toolchain_project_name | get-menu -path "Configure/Configure as Yocto Project Autotools project (alpha)"
	        | click
	}
	with [get-window [concat "Properties for " $autotools_sdk_toolchain_project_name " (Filtered)"]] {
	    get-group "Cross development profile" | get-combo -after [get-label "Profile:"] | select $sdk_toolchain_profile_name
	    get-button "Apply and Close" | click
	}
	get-view "Project Explorer" | get-tree | get-menu -path Refresh | click
	try{
  	get-view Console | get-button "Display Selected Console" | click -arrow
  	get-menu -path "1 CDT Global Build Console" | click
  	with [get-view Console] {
  	    get-button "Pin Console" | click
  	    get-editbox -after [get-label "CDT Global Build Console"] | get-menu -path Clear | click
  	}
	}-catch{}
	get-view "Project Explorer" | get-tree | select [concat $autotools_sdk_toolchain_project_name " [" $sdk_toolchain_profile_name "]" ] | get-menu
	    -path "Reconfigure Project" | click
	get-view Console | get-text-viewer | get-property text | contains $gcc_validation | verify-true

	try{
  	get-view Console | get-button "Display Selected Console" | click -arrow
  	get-menu -path "1 CDT Global Build Console" | click
  	with [get-view Console] {
  	    get-button "Pin Console" | click
  	    get-editbox -after [get-label "CDT Global Build Console"] | get-menu -path Clear | click
  	}
	}-catch{}
	get-view "Project Explorer" | get-tree | select [concat $autotools_sdk_toolchain_project_name " [" $sdk_toolchain_profile_name "]" ] | get-menu
	    -path "Build Project" | click
	get-view Console | get-text-viewer | get-property text | contains $gcc_validation | verify-true
}-else{
	echo "Test case \"Build Autotools SDK Toolchain Project\" skipped. Parameter SDK_Path not set."
}
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
